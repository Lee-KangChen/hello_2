<!-- app/views/shared/_navbar.html.erb -->
<ul>
  <%= nav_li 'Home', root_path %>
  <%= nav_li 'About', about_path %>
  <%= nav_li 'Lotto', math_path %>
  <%= nav_li 'Form', form_path %>
  <%= nav_li 'All Post', posts_path %>
  <%= nav_li 'New Post', new_post_path %>
  <%= nav_li 'Category', categories_path %>
</ul>

<% if user_signed_in? %>
  Hello, <%= current_user.name %> |
  <%= link_to "logout", session_path, method: :destroy %>
<% else %>
  <%= form_tag session_path do %>
    <input type="text" name="email">
    <input type="password" name="password">
    <input type="submit" value="login">
  <% end %>
<% end %>
<% line_number.times do %>
  <hr>
<% end %>
